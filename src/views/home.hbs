<div >

    <div>
        
        {{#each datos }}
        
        <div class="contContactos">
            <div class="Cont_p"><h1 id="titulo_contactos">{{nombre}}</h1></div>
            <div class="Cont_p">{{numero}} </div>
            <div class="Cont_p">{{descripcion}} </div>
            
            <div >
                <div class="cont-boton">
                    <a href="/editar/{{idP}}/{{id}}"class="boton_cont">editar</a>
                </div>
                <div class="cont-boton"> 
                    <a href="/eliminar/{{idP}}/{{id}}" class="boton_cont">eliminar</a>
                </div>
            </div>
            
        </div>
        
        {{else}}

        <div class="cont_vacio"> 
            <div>
                <div>
                    <h2 style="color: black;"> Actualmente no tienes contactos</h2>
                </div>
                <div class="agregar" >
                     <a  class="agre" href="/agregar/{{idP}}">agregar uno</a>
                </div>
               
            </div>
        </div>
        
        
        {{/each}}
    </div>
</div>
